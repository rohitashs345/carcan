<script>
  $(document).ready(function(){
    var newVal=2;
    var ValueLeft;
    $(".quantity__left").click(function(){
      var $button = $(this);

      if ($button.data('direction') == "-1") {
        if (newVal > 2) {
          newVal--;
          ValueLeft = 5 - newVal;
        } else {
          newVal = 2;
        }
      } 
      else {
        newVal++;
        ValueLeft = 5 - newVal;
      }


      if (newVal == 4) {
        $("#items-left").html( ValueLeft + " CARCAN Away From FREE SHIPPING").css({"color": "#454545", "font-size": "12px"});
      }
      else if (newVal < 4) {
        $("#items-left").html( ValueLeft + " CARCANS Away From FREE SHIPPING").css({"color": "#454545", "font-size": "12px"});
      }
      else {
        $("#items-left").html('FREE SHIPPING APPLIED <i class="far fa-check-circle"></i>').css("color", "#19af42").animate({fontSize: '14px'}, "slow");  
      }


    });
  });
</script>

